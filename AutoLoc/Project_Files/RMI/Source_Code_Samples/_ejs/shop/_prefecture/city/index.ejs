<% const pagetitle = "楽天モバイル 二子玉川店" -%>
<%-
 include('/_header', {
    pagetitle: pagetitle,
    description: '',
    keywords: '',
    noindex: false,
    pathname: filepath
  });
%>
<%- include('/include/_common-css') %>
<%- include('/include/shop/_category-css') %>
<%- include('/include/_append-head') %>
</head>
<body>
<%- include('/include/_prepend-body') %>

<div class="l-System">

<%- include('/include/_header') %>

<div class="c-Breadcrumbs_Head">
  <ul>
    <li><a href="<%= rootpath %>"><%= sitetitle %></a><span class="c-Icon_Chevron-right"></span></li>
    <li><a href="<%= sitepath %>shop/">ショップ</a><span class="c-Icon_Chevron-right"></span></li>
    <li area-current="true"><%= pagetitle %></li>
  </ul>
</div>

<div class="c-Heading_Hero-basic">
  <div class="l-System_Container">
    <div class="c-Heading_Hero-ttl">
      <span class="shop-Detail_Label-header c-Txt_Alphanumeric-s">12/27 NEW OPEN</span>
      <h1 class="c-Heading_Lv1"><%= pagetitle %></h1>
    </div>
  </div>
</div>

<div class="shop-Layout_Detail-main">
  <div class="l-System_Container">
    <div class="shop-Detail_Contents">
      <div class="shop-Map test" data-shopname="楽天モバイル 二子玉川店" data-lat="35.61034421822453" data-lng="139.6299438888889">
        <div class="shop-Map_Container test">
          <iframe src="https://maps.google.co.jp/maps?&output=embed&q=35.61034421822453,139.6299438888889" width="600" height="240" frameborder="0" style="border:0" allowfullscreen></iframe>
        </div>
        <ul class="shop-Map_Btns">
          <li><a href="https://www.google.com/maps?q=35.61034421822453,139.6299438888889" class="c-Btn_Regular-auto shop-Map_Btn-gmap test" target="_blank"><span>Google mapで開く</span></a></li>
          <li><a href="#submap" class="c-Btn_Regular-auto js-Modal"><span>案内図を見る</span></a></li>
        </ul>
        <div id="submap" style="display:none;">
          <p class="shop-Map_Sub">
            <img data-src="https://dummyimage.com/600x600/d1d1d1/fff.png&text=SubMap" class="js-lazy" alt="">
          </p>
        </div>
      </div>

      <ul class="shop-Bnr_Top">
        <li><a href="#"><img src="<%= sitepath %>assets/img/bnr/bnr_sample-2.png" width="300" height="100" alt=""></a></li>
      </ul>

      <div class="shop-Detail_Info">
        <div class="c-Infobox_Standing-info">
          <p>10/5は、館内点検のため、休業となります。予めご了承ください。</p>
        </div>
      </div>

      <div class="shop-Reservation" data-reservationkey="KR00179660">

        <%- include('/include/shop/_flow') %>

        <div class="shop-Reservation_Head">
          <h2 class="shop-Reservation_Title">来店予約</h2>
          <p class="shop-Reservation_Waiting">（ただ今の待ち人数:<span id="waitcount">1組</span>）</p>
        </div>
        <div class="shop-Reservation_Body">
          <div>
            <a href="#" class="c-Btn_Secondly-auto shop-Reservation_Btn"><span>Webで予約する（翌日以降）</span></a>
          </div>
          <div class="shop-Reservation_Tel">
            <p class="shop-Reservation_Tel-title">当日のご予約はお電話にて</p>
            <a href="tel:xxxxxxxxxxx" class="c-Txt_Alphanumeric">xxx-xxxx-xxxx</a>
          </div>
          <p class="shop-Reservation_Time">予約受付時間：（全日11:00〜18:00）</p>
          <p class="c-Txt_Cap u-Adjust_Mt-8">※注釈テキストがはいります。</p>
        </div>
        <div class="shop-Reservation_Foot">
          <a href="<%= sitepath %>guide/1.html" class="c-Btn_Regular-auto shop-Reservation_Link"><span>ご来店時に持参いただく物はこちら</span></a>
        </div>
      </div>
      <div class="shop-Reservation" data-reservationkey="KR00179660">

