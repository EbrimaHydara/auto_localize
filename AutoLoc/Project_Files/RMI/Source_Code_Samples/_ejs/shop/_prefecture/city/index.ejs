      <li><a href="<%= sitepath %>shop/">ショップ</a><span class="c-Icon_Chevron-right"></span></li>
      <li area-current="true"><%= pagetitle %></li>
    </ul>
  </div>

  <%- include('/include/_footer') %>

</footer>


</div>
<%- include('/include/_common-js') %>
