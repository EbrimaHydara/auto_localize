<% const namespace = "ecare" -%>
<!---  other -->
<div class="c-Accordion">
  <button type="button" id="<%= prefix %>accordion-member-other" class="c-Accordion_Trigger js-Accordion_Trigger <%= namespace %>-Accordion_Parent" aria-expanded="false" aria-controls="<%= prefix %>accordion-content-member-other" data-effect="false" data-ratid="ecare-s-<%= type %>_category_member_other" data-ratevent="click" data-ratparam="all"><span class="c-Icon_Meatball-horizontal-f"></span>その他<span class="c-Icon_Chevron-right <%= namespace %>-Accordion_Arrow"></span></button>
