<div class="c-Carousel <%= namespace %>-Carousel_Wrap js-<%= namespace %>-Carousel">
<%_
  items.forEach(function (item) {
  const { model, lead, features, img, link, is5g, colors, capacitys, isNew } = item
  const imgWith5g = is5g ? `${namespace}-Carousel_Img-5g` : null
  const imgAdjustMt = item.isAdjustImg ? ' u-Adjust_Mt-0' : null
_%>

  <a class="c-Card_Normal <%= namespace %>-Carousel_Item" href="<%= sitepath %><%= link %>">
    <div class="<%= namespace %>-Carousel_Item-inner">
      <div class="<%= namespace %>-Carousel_Item-header">

        <div class="<%= namespace %>-Carousel_New">
        <%_ if (isNew) { _%>
          <div class="<%= namespace %>-Carousel_New-icon"><span>NEW</span></div>
        <%_ } _%>
        </div>

        <h4 class="u-Adjust_Align-center u-Adjust_Mt-8"><%= model %></h4>
        <%_ if (typeof lead !== 'undefined') { _%>
          <p class="c-Txt_Emp-01 u-Adjust_Align-center"><%- lead %></p>
        <%_ } _%>

      </div>
      <div class="<%= namespace %>-Carousel_Item-body">

        <%_ if (typeof features !== 'undefined') { _%>
          <ul class="<%= namespace %>-Carousel_Features">
          <%_ features.forEach(function (feature) { _%>
            <li><span class="c-Icon_Check"></span><%- feature %></li>
          <%_ }) _%>
          </ul>
        <%_ } _%>


        <figure>
          <img src="<%= sitepath %><%= img %>" alt="<%= model %>" class="<%= imgWith5g %><%= imgAdjustMt %>">
        </figure>

        <ul class="<%= namespace %>-Carousel_ItemColor">
        <%_
          colors.forEach(function (color) {
          const { color: bgColor, isBorder } = color
          const borderClass = isBorder ? 'color-white' : null
        _%>
          <li><span class="<%= borderClass %>" style="background: <%= bgColor %>;"></span></li>
        <%_ }) _%>
        </ul>

        <%_ if (typeof capacitys !== 'undefined') { _%>
          <ul class="<%= namespace %>-Carousel_Capacitys">
          <%_ capacitys.forEach(function (capacity) { _%>
            <li><span><%= capacity %></span></li>
          <%_ }) _%>
          </ul>
        <%_ } _%>

      </div>
    </div>
  </a>

<%_ }) _%>
</div>
<div class="c-Carousel_Nav-v2-wrap">
  <div class="c-Carousel_Nav-v2"></div>
  <div class="c-Carousel_Nav-v2-bar"></div>
</div>
