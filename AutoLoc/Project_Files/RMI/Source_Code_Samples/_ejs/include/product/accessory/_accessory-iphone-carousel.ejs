
<section class="accessory-detail-Carousel" id="recommend">
  <div class="l-System_Container">
    <h2 class="c-Heading_Lv2 u-Adjust_Align-center">おすすめアクセサリ</h2>
    <div class="u-Adjust_Mt-16">
      <div class="c-Carousel accessory-detail-Carousel_Wrap js-accessory-detail-Carousel-product">

        <%_ accessoryArray.forEach(value => { _%>
          <a class="c-Card_Normal accessory-detail-Carousel_Item" href="<%- value.url %>">
            <div class="accessory-detail-Carousel_Flex">
              <div class="accessory-detail-Carousel_Img">
                <img src="<%- value.image %>" alt="<%- value.name %>">
              </div>
              <div class="accessory-detail-Carousel_Txt">
                <p class="accessory-detail-Carousel_Name"><%- value.name %></p>
                <%_ if(value.color) { _%>
                  <ul class="accessory-detail-Carousel_Color">
                    <%_ value.color.forEach(ele => { _%>
                      <li>
                        <span class="" style="background: <%- ele.code %>;"></span>
                      </li>
                    <%_ }); _%>
                  </ul>
                <%_ } _%>
              </div>
            </div>
            <div>
              <p class="accessory-detail-Carousel_Price"><span><%- value.price %></span><span class="accessory-detail-Carousel_Price-yen">円</span></p>
            </div>
          </a>
        <%_ }); _%>

      </div>
      <div class="c-Carousel_Nav-v2-wrap">
        <div class="c-Carousel_Nav-v2"></div>
        <div class="c-Carousel_Nav-v2-bar"></div>
      </div>
    </div>
  </div>
</section>
