<body>
<%- include('/include/_prepend-body') %>

<div class="l-System">

<%- include('/include/_header') %>

<div class="c-Breadcrumbs_Head">
  <ul>
    <li><a href="<%= rootpath %>"><%= sitetitle %></a><span class="c-Icon_Chevron-right"></span></li>
    <%_ if (typeof directories !== 'undefined') { _%>
    <%_ let dirs = []; _%>
    <%_ directories.forEach(function (dir) { _%>
      <%_ if (dir.path !== "") { _%>
        <li><a href="<%= sitepath %><%= dir.path %>"><%= dir.label %></a><span class="c-Icon_Chevron-right"></span></li>
      <%_ } else { _%>
        <li><%= dir.label %><span class="c-Icon_Chevron-right"></span></li>
      <%_ } _%>
    <%_ }) _%>
    <%_ } _%>
    <li aria-current="true"><%= pagetitle %></li>
  </ul>
</div>

<div class="<%= namespace %>-Layout_Hero">
  <div class="l-System_Container">
    <p class="c-Txt_Cap u-Adjust_Align-right">※表記の金額はすべて税込です。</p>
  </div>
  <div class="l-System_Container u-Adjust_Mt-24">
    <p class="u-Adjust_Align-center">
      <span class="c-Txt_Emp-01 c-Txt_Normal-l">Apple Watch Nike Series 7は、販売を終了いたしました。</span>
    </p>
    <div class="u-Adjust_Mt-16 u-Adjust_Align-center"><a class="c-Btn_Regular" href="<%= sitepath %>product/apple-watch/?l-id=end_product_apple-watch">販売中のApple Watch製品を見る</a></div>
  </div>
  <div class="l-System_Container">
    <div class="<%= namespace %>-Utility_Disp-sp u-Adjust_Mt-16">
      <h1 class="c-Heading_Lv1 <%= namespace %>-Hero_Name">Apple Watch<br> Nike Series 7</h1>
      <div class="<%= namespace %>-Hero_Reference u-Adjust_Mt-8 u-Adjust_Mb-24">
        <a href="<%= sitepath %>product/apple-watch/comparison/?l-id=apple-watch-nike-series7_top_apple-watch_comparison" class="c-Link_Txt-inline-icon js-scroll">ほかのApple Watchと比較する<span class="c-Icon_Chevron-right"></span></a>
      </div>
    </div>
    <section class="<%= namespace %>-Hero">
      <div class="<%= namespace %>-Hero_Main">
        <%_ productInfo.slider.forEach((value, index) => { _%>
          <%_ if( productInfo.color1.length <= 0 ) { _%>
            <ul class="<%= namespace %>-Hero_Main-view c-Carousel js-product-main js-device">
          <%_ } else if( index === 0 ) { _%>
            <ul class="<%= namespace %>-Hero_Main-view c-Carousel js-main<%- index + 1 %> js-device" aria-hidden="false">
          <%_ } else { _%>
            <ul class="<%= namespace %>-Hero_Main-view c-Carousel js-main<%- index + 1 %> js-device" aria-hidden="true">
          <%_ } _%>
            <%_ value.forEach((ele, index) => { _%>
              <li><span><img data-lazy="<%- ele %>" alt=""></span></li>
            <%_ }); _%>
            </ul>

          <%_ if( index === 0 ) { _%>
            <ul class="<%= namespace %>-Hero_Main-thumbnail thumbnail<%- index + 1 %> is-open">
          <%_ } else { _%>
            <ul class="<%= namespace %>-Hero_Main-thumbnail thumbnail<%- index + 1 %>">
          <%_ } _%>
            <%_ value.forEach((ele, index) => { _%>
              <li class="thumbnail-item"><span><img src="<%- ele %>" alt=""></span></li>
            <%_ }); _%>
            <%_ if( productInfo.movie.flag ) { _%>
              <li>
                <a class="js-video" href="<%- productInfo.movie.url %>">
                  <span><img src="<%- productInfo.movie.thumbnail %>" alt=""></span>
                </a>
              </li>
            <%_ } _%>
            </ul>
        <%_ }); _%>
        <p class="c-Txt_Cap u-Adjust_Align-center u-Adjust_Mt-16 u-Adjust_Mb-40">※GPS + Cellularモデルのみの取扱いとなります。</p>
        <div class="c-Carousel_Nav <%= namespace %>-Layout_Carousel"></div>
      </div>

      <div class="<%= namespace %>-Hero_Data">
        <div>
          <div class="<%= namespace %>-Layout_PC u-Adjust_Mt-16">
            <h1 class="c-Heading_Lv1 <%= namespace %>-Hero_Name"><%- productInfo.name %></h1>
            <div class="<%= namespace %>-Hero_Reference u-Adjust_Mt-8 u-Adjust_Mb-24">
              <a href="<%= sitepath %>product/apple-watch/comparison/?l-id=apple-watch-nike-series7_top_apple-watch_comparison" class="c-Link_Txt-inline-icon">ほかのApple Watchと比較する<span class="c-Icon_Chevron-right"></span></a>
            </div>
          </div>
          <h4 class="c-Heading_Lv4">ケースのサイズ</h4>
          <ul class="<%= namespace %>-Hero_Casesize-list u-Adjust_Mt-16">
            <li>
              <label class="c-Form_Radioblock-vertical">
                <input type="radio" name="casesize" value="41" checked autocomplete="off">
                <span class="c-Form_Radioblock-wrap <%= namespace %>-Hero_Casesize-list-wrap">
                  <span class="c-Form_Radio-icon"></span><span class="c-Form_Radioblock-label <%= namespace %>-Hero_Casesize-list-label">41mm</span>
                </span>
              </label>
            </li>
            <li>
              <label class="c-Form_Radioblock-vertical">
                <input type="radio" name="casesize" value="45" autocomplete="off">
                <span class="c-Form_Radioblock-wrap <%= namespace %>-Hero_Casesize-list-wrap">
                  <span class="c-Form_Radio-icon"></span><span class="c-Form_Radioblock-label <%= namespace %>-Hero_Casesize-list-label">45mm</span>
                </span>
              </label>
            </li>
          </ul>
          <h4 class="c-Heading_Lv4 u-Adjust_Mt-24">ケース</h4>
          <%_ if( productInfo.color1.length > 0 ) { _%>
            <ul class="<%= namespace %>-Hero_Color-1 u-Adjust_Mt-16">
              <%_ productInfo.color1.forEach((value, index) => { _%>
                <%_ if( index === 0 ) { _%>
                  <li class="js-color-m js-color<%- index + 1 %>" data-casetype="<%- value.name %>" aria-current="true">
                <%_ } else { _%>
                  <li class="js-color-m js-color<%- index + 1 %>" data-casetype="<%- value.name %>" aria-current="false">
                <%_ } _%>
                  <%_ if( value.name === 'ホワイト' ) { _%>
                    <div style="background: <%- value.code %>; border: 1px solid #D1D1E0;"></div>
                    <p><%- value.output %></p>
                  <%_ } else if ( value.name === '(PRODUCT)RED' ) { _%>
                    <div style="background: <%- value.code %>"></div>
                    <p class="u-Adjust_Mt-8" >（PRODUCT）<br>RED&#8482;</p>
                  <%_ } else { _%>
                    <div style="background: <%- value.code %>"></div>
                    <p><%- value.output %></p>
                  <%_ } _%>
                  </li>
              <%_ }); _%>
            </ul>
          <%_ } _%>
          <h4 class="c-Heading_Lv4 u-Adjust_Mt-24">バンド</h4>
          <ul class="<%= namespace %>-Hero_Band-list u-Adjust_Mt-16">
            <li class="<%= namespace %>-Hero_Band-list-elem" aria-hidden="false">
              <label class="c-Form_Radioblock-vertical">
                <input type="radio" name="band" value="アンスラサイト/ブラックNikeスポーツバンド" checked autocomplete="off">
                <span class="c-Form_Radioblock-wrap <%= namespace %>-Hero_Band-list-wrap">
                  <span class="c-Form_Radio-icon"></span><span class="c-Form_Radioblock-label <%= namespace %>-Hero_Band-list-label">アンスラサイト/ブラックNikeスポーツバンド</span>
                </span>
              </label>
            </li>
            <li class="<%= namespace %>-Hero_Band-list-elem" aria-hidden="true">
              <label class="c-Form_Radioblock-vertical">
                <input type="radio" name="band" value="ピュアプラチナム/ブラックNikeスポーツバンド" autocomplete="off">
                <span class="c-Form_Radioblock-wrap <%= namespace %>-Hero_Band-list-wrap">
                  <span class="c-Form_Radio-icon"></span><span class="c-Form_Radioblock-label <%= namespace %>-Hero_Band-list-label">ピュアプラチナム/ブラックNikeスポーツバンド</span>
                </span>
              </label>
            </li>
          </ul>

          <div class="u-Rat_Appear-height" data-ratid="<%= currentDir %>_scroll-00_price" data-ratevent="appear">&nbsp;</div>
        </div>
      </div>
    </section>
  </div>
</div>

<div class="l-System_Container u-Adjust_Mb-16">
  <%_ const bssId = '50150809' _%>
  <%- include('/include/product/apple-watch/spec/_series7', { namespace, currentDir: 'apple-watch-series7', deviceName: 'nike-series7', ratidDir: 'apple-watch-nike-series7' }) %>
</div>

<%- include('/include/product/apple-watch/_apple-watch-list', { l_id: 'apple-watch-nike-series7', currentDir }) %>

<footer class="g-Footer">

  <div class="g-Footer_Aside">

    <%- include('/include/footer/related/_related-product-v2.ejs') %>

    <%- include('/include/footer/bnr/_bnr.ejs') %>

  </div>
  <div class="l-System_Container">
    <div class="c-Breadcrumbs_Foot">
      <ul>
        <li><a href="<%= rootpath %>"><%= sitetitle %></a><span class="c-Icon_Chevron-right"></span></li>
        <%_ if (typeof directories !== 'undefined') { _%>
        <%_ let dirs = []; _%>
        <%_ directories.forEach(function (dir) { _%>
          <%_ if (dir.path !== "") { _%>
            <li><a href="<%= sitepath %><%= dir.path %>"><%= dir.label %></a><span class="c-Icon_Chevron-right"></span></li>
          <%_ } else { _%>
            <li><%= dir.label %><span class="c-Icon_Chevron-right"></span></li>
          <%_ } _%>
        <%_ }) _%>
        <%_ } _%>
        <li aria-current="true"><%= pagetitle %></li>
      </ul>
    </div>
  </div>

  <%- include('/include/_footer') %>

</footer>

</div>
<%- include('/include/_common-js') %>
<%- include('/include/product/_category-js') %>
<%- include('/include/_append-body') %>
<script src="<%= sitepath %>assets/js/product.apple-watch.apple-watch-nike-series7.index.bundle.js?20240521"></script>

</body>
</html>
