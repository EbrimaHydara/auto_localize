<script>
(function($){
  $(function(){
    const selectCheck = function() {
      let colorflag = false;
      let planflag = false;
      const colorSelect = $('input[name="color"]');
      const planSelect = $('input[name="sim_plan"]');

      for ( let i = 0; i < colorSelect.length; i++ ) {
        if( colorSelect[i].checked ) colorflag = true;
      }
      for ( let i = 0; i < planSelect.length; i++ ) {
        if( planSelect[i].checked ) planflag = true;
      }
      if ( colorflag && planflag ) {
        $('.js-apply').attr({ 'href': onboadingUrlSelect(), 'aria-disabled': 'false' });
      }
    }

    const onboadingUrlSelect = function() {
      const selectColor = $('input[name="color"]:checked').val();
      const selectPlan = $('input[name="sim_plan"]:checked').val();
      const value = selectColor +','+ selectPlan;
      let onboadingUrl = 'https://portal.mobile.rakuten.co.jp/referral_link'

      switch (value) {
        case 'color1,e-sim':
          onboadingUrl = 'https://portal.mobile.rakuten.co.jp/referral_link?id=9158206969665397225:1&id_d=9158806889765638043:0';
          break;
        case 'color2,e-sim':
          onboadingUrl = 'https://portal.mobile.rakuten.co.jp/referral_link?id=9158206969665397225:1&id_d=9158806889765638072:0';
          break;
        case 'color1,p-sim':
          onboadingUrl = 'https://portal.mobile.rakuten.co.jp/referral_link?id=9158206969665397225:0&id_d=9158806889765638043:0';
          break;
        case 'color2,p-sim':
          onboadingUrl = 'https://portal.mobile.rakuten.co.jp/referral_link?id=9158206969665397225:0&id_d=9158806889765638072:0';
          break;
        case 'color1,no-plan':
          onboadingUrl = 'https://portal.mobile.rakuten.co.jp/equipment-details?id=9158806889765638043';
          break;
        case 'color2,no-plan':
          onboadingUrl = 'https://portal.mobile.rakuten.co.jp/equipment-details?id=9158806889765638072';
          break;
        default:
          break;
      }
      return onboadingUrl;
    }

    $('.js-choice').on('change', function() {
      selectCheck();
    });
  });
})(jQuery);
</script>
