<% const pagetitle = "FUT端末の返却について" -%>
<% const directories = [{path:"info/", label:"お知らせ"}] -%>
<%-
 include('/_header', {
    pagetitle: pagetitle,
    directories: directories,
    description: 'FUT（Friendly User Test）で使用した端末の返却先、返却方法はこちらをご参照ください。',
    noindex: true,
    pathname: filepath
  });
%>
<%- include('/include/_common-css-v2') %>
<%- include('/include/_append-head') %>
</head>
<body>
<%- include('/include/_prepend-body') %>

<div class="l-System">

<%- include('/include/_header') %>

<div class="c-Breadcrumbs_Head">
  <ul>
    <li><a href="<%= rootpath %>"><%= sitetitle %></a><span class="c-Icon_Chevron-right"></span></li>
    <%_ if (typeof directories !== 'undefined') { _%>
    <%_ let dirs = []; _%>
    <%_ directories.forEach(function (dir) { _%>
      <%_ if (dir.path !== "") { _%>
        <li><a href="<%= sitepath %><%= dir.path %>"><%= dir.label %></a><span class="c-Icon_Chevron-right"></span></li>
      <%_ } else { _%>
        <li><%= dir.label %><span class="c-Icon_Chevron-right"></span></li>
      <%_ } _%>
    <%_ }) _%>
    <%_ } _%>
    <li aria-current="true"><%= pagetitle %></li>
  </ul>
</div>

<div class="l-System_Container">
    <div>
        <h1 class="c-Heading_Lv1 u-Adjust_Mt-40">FUT端末の返却について</h1>
        <p class="u-Adjust_Mt-24">FUT（Friendly User Test）で使用した端末の返却先、返却方法は以下をご参照ください。</p>
    </div>
     <div class="u-Adjust_Mt-24 u-Adjust_Align-left">
       <h2 class="c-Heading_Lv2 u-Adjust_Mt-24">返却先</h2>
         <p class="u-Adjust_Mt-16">
                〒158-0094 東京都世田谷区玉川1-14-1 <br>
                楽天クリムゾンハウス楽天モバイル（株）<br>
                ネットワーク統括本部 <br>
                Device & Terminal Eng.部 村田 宛
        </p>
       <h2 class="c-Heading_Lv2 u-Adjust_Mt-32">返却方法</h2>
         <ul class="c-List_Text-disc u-Adjust_Mt-16">
           <li>社内便</li>
           <li>宅配便・ゆうパック（着払い不可）</li>
         </ul>
        <h2 class="c-Heading_Lv2 u-Adjust_Mt-32">対象機種</h2>
          <ul class="c-List_Text-disc u-Adjust_Mt-16">
            <li>Huawei P30 Lite/86694604xxxxxxx（返却必須）</li>
            <li>Wiko Tommy 3＋/352874100xxxxxxx（処分に困る場合は返却ください）</li>
          </ul>
    </div>
</div>



<footer class="g-Footer">

    <div class="l-System_Container">
      <div class="c-Breadcrumbs_Foot">
        <ul>
          <li><a href="<%= rootpath %>"><%= sitetitle %></a><span class="c-Icon_Chevron-right"></span></li>
          <%_ if (typeof directories !== 'undefined') { _%>
          <%_ let dirs = []; _%>
          <%_ directories.forEach(function (dir) { _%>
            <%_ if (dir.path !== "") { _%>
              <li><a href="<%= sitepath %><%= dir.path %>"><%= dir.label %></a><span class="c-Icon_Chevron-right"></span></li>
            <%_ } else { _%>
              <li><%= dir.label %><span class="c-Icon_Chevron-right"></span></li>
            <%_ } _%>
          <%_ }) _%>
          <%_ } _%>
          <li aria-current="true"><%= pagetitle %></li>
        </ul>
      </div>
    </div>
    <%- include('/include/_footer') %>

  </footer>

</div>
<%- include('/include/_common-js') %>
<%- include('/include/_append-body') %>
</body>
</html>
