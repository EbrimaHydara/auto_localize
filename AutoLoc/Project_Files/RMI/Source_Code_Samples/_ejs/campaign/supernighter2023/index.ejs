  <script>
    (function(){
      const tabs = document.querySelectorAll('.js-tab-nest');
      const panels = document.querySelectorAll('.js-panel-nest');
      tabs.forEach(tab => {
        tab.addEventListener('click', e => {
          let self = e.currentTarget;
          let target= document.getElementById(self.getAttribute('aria-controls'));

          tabs.forEach(tab => {
              tab.setAttribute('aria-selected', false);
          });
          panels.forEach(panel => {
              panel.setAttribute('aria-hidden', true);
          });
          self.setAttribute('aria-selected', true);
          target.setAttribute('aria-hidden', false);
        });
      });
    })();
    </script>
