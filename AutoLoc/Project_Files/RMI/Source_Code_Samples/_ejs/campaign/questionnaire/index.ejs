<% const pagetitle = "楽天モバイルに関するアンケートに答えるだけで100ポイントもらえる！キャンペーン" -%>
<% const directories = [{path:"campaign/", label:"キャンペーン・特典"}] -%>
<% const namespace = "questionnaire" -%>
<%-
include('/_header', {
  pagetitle: pagetitle,
  directories: directories,
  description: '楽天モバイルに関するアンケートに答えるだけで100ポイントもらえます。',
  keywords: '',
  noindex: false,
  pathname: filepath
});
%>
<%- include('/include/_common-css-v2') %>
<link rel="stylesheet" href="<%= sitepath %>assets/css/campaign/promotion/questionnaire/questionnaire.css?20240424">
