  <script>
    (function ($) {
    $(function () {
      var $checkbox = $('#tokuregi-agree');
      var $error = $('.js-tokuregi-error');
      $('.js-tokuregi-agree').on('click', function (e) {
          if (!$checkbox.prop('checked')) {
            e.preventDefault();
            e.stopPropagation();
            $error.hide();
            $(this).prev('.js-tokuregi-error').show();
          }
      });
      $checkbox.on('change', function () {
        $error.hide();
      });
    });
  })(window.jQuery);
