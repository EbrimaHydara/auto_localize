    // });

  function getParam(name, url) {
    if (!url) url = window.location.href;
    name = name.replace(/[\[\]]/g, "\\$&");
    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
      results = regex.exec(url);
    if (!results) return null;
    if (!results[2]) return '';
    return decodeURIComponent(results[2].replace(/\+/g, " "));
  }

  var prm;
  var carttype = (getParam('cart_type'));
  var coupon = getParam('coupon');
  // console.log(carttype);
  // console.log(coupon);

  if (carttype === null || coupon === null) {
    $('.tokuregi-notice-error').css('display', 'block');
    $('#agree').prop('disabled','false');
  } else if (carttype == 2){
    $('.js-link').html('<a href="https://ts.sp.basket.step.rakuten.co.jp/rms/mall/bss/morderfrom/?status=4&scid=tokreg_mno_mvno-back2baskets" class="c-Btn_Regular-large <%= namespace %>-Button-Regular" data-ratId="tokreg_mno_mig-notice_back_footer" data-ratEvent="click" data-ratParam="all"><span>お申し込みせず楽天市場でのお買い物に戻る</span></a>');
  } else {
  }

  function retrieveGETqs() {
    var query = window.location.search.substring(1);
    return query;
    if (!query) return false;
  }
  $(function () {
    $('.js-param').each(function () {
        var $this = $(this);
        var target_url = $this.attr("href");

        if (target_url.indexOf('?') !== -1) {
            $this.attr('href', target_url + '&' + "cart_type=" + carttype + "&coupon=" + coupon);
        } else {
            $this.attr('href', target_url + '?' + "cart_type=" + carttype + "&coupon=" + coupon);
        }
    });
});
</script>


