</div>
<script type="text/javascript">
  function doTrim(el) {
    el.value = el.value.replace(/^\s+|\s+$/g, '');
  }
  function isInput(el) {
    return el.value.length != 0;
  }

  function isLength(el, len) {
    return el.value.length === len;
  }
  function isNumber(el) {
    return (el.value.match(/^[0-9]+$/)) ? true : false
  }
  function zen2han(el) {
    el.value = el.value.replace(/[０]/g, "0");
    el.value = el.value.replace(/[１]/g, "1");
    el.value = el.value.replace(/[２]/g, "2");
    el.value = el.value.replace(/[３]/g, "3");
    el.value = el.value.replace(/[４]/g, "4");
    el.value = el.value.replace(/[５]/g, "5");
    el.value = el.value.replace(/[６]/g, "6");
    el.value = el.value.replace(/[７]/g, "7");
    el.value = el.value.replace(/[８]/g, "8");
    el.value = el.value.replace(/[９]/g, "9");
  }
  function checkForm() {
    var el;
    var obj = document.enenForm;

    el = obj.elements['answers[39968_272324]'];
    doTrim(el);
    zen2han(el);
    if (!isInput(el)) {
      alert("「回線識別番号」を入力してください。");
      return false;
    }
    if (!isLength(el, 4)) {
      alert("「回線識別番号」は4桁で入力してください。");
      return false;
    }
    if(!isNumber(el)) {
      alert("「回線識別番号」は半角数字で入力してください。");
      return false;
    }
  }

  const elAgree = document.getElementById('js-agree');
  const submitBtn = document.getElementById('js-submit');
  elAgree.addEventListener('change', () => {
    if( elAgree.checked ) {
      submitBtn.setAttribute('aria-disabled', false);
      submitBtn.disabled = false;
    } else {
      submitBtn.setAttribute('aria-disabled', true);
      submitBtn.disabled = true;
    }
  })
</script>
