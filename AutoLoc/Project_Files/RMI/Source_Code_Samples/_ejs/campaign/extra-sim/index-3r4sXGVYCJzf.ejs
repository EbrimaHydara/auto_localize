<% const pagetitle = "entry test" -%>
<% const directories = [{path:"campaign/", label:"キャンペーン・特典"}] -%>
<% const namespace = "extra-sim" -%>
<%-
include('/_header', {
  pagetitle: pagetitle,
  directories: directories,
  description: '',
