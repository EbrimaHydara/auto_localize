 include('/_header', {
    pagetitle: pagetitle,
    description: 'Rakuten UN-LIMIT VII 紹介プログラム 紹介コード入力フォームです。',
    keywords: '',
    noindex: true,
    pathname: filepath
  });
%>
<%- include('/include/_common-css-v2') %>
<link rel="stylesheet" href="<%= sitepath %>assets/css/campaign/promotion/rakuten-employee-2210/rakuten-employee-2210.css?20240424">
<%- include('/include/_append-head') %>

<script type="text/javascript">
  function doTrim(el) {
    el.value = el.value.replace(/^\s+|\s+$/g, '');
  }

  function isInput(el) {
    return el.value.length != 0;
  }

  function isLength(el, len) {
    if ("え".length == 2) {
      len *= 2;
    }
    return el.value.length == len;
  }

  function zen2han(el) {
    el.value = el.value.replace(/[０]/g, "0");
    el.value = el.value.replace(/[１]/g, "1");
    el.value = el.value.replace(/[２]/g, "2");
    el.value = el.value.replace(/[３]/g, "3");
    el.value = el.value.replace(/[４]/g, "4");
    el.value = el.value.replace(/[５]/g, "5");
    el.value = el.value.replace(/[６]/g, "6");
    el.value = el.value.replace(/[７]/g, "7");
    el.value = el.value.replace(/[８]/g, "8");
    el.value = el.value.replace(/[９]/g, "9");
  }

  function checkForm() {
    var el;
    var obj = document.enenForm;

    el = obj.elements['answers[38692_264610]'];
    doTrim(el);
    zen2han(el);
    if (!isInput(el)) {
      alert("「英字2ケタ＋数字5ケタ」の紹介コードを入力してください。");
      return false;
    }

    if (!isLength(el, 7)) {
      alert("「英字2ケタ＋数字5ケタ」の紹介コードを入力してください。");
      return false;
    }

  }
  </script>

</head>
<body>
<%- include('/include/_prepend-body') %>

<div class="l-System">

