          $('.l-System').css({
            position: 'relative',
            top:''
          });

          body.css({
            position: 'relative',
            top:''
          })

          setTurboTab();
        });

        // リファラの確認
        var referrer = document.referrer;
        if (referrer.indexOf('/hikari/') !== -1) {
          setHikariTab();
        }

        // URLパラメータのチェック
        if (window.location.href.indexOf('?tab-list=tab-menu2') !== -1) {
          setHikariTab();
        }
      });


      // 高さを揃える
      $(window).on("load resize", function () {
        var maxHeightTop = 0;
        var maxHeightBottom = 0;

        $(".js-featureBoxTopHeight").height("auto");
        $(".js-featureBoxBottomHeight").height("auto");

        $(".js-featureBoxTopHeight").each(function () {
          var height = $(this).height();
          if (height > maxHeightTop) {
            maxHeightTop = height;
          }
        });

        $(".js-featureBoxBottomHeight").each(function () {
          var height = $(this).height();
          if (height > maxHeightBottom) {
            maxHeightBottom = height;
          }
        });

        $(".js-featureBoxTopHeight").height(maxHeightTop);
        $(".js-featureBoxBottomHeight").height(maxHeightBottom);
      });

      (function ($) {
        $('#SPU_start-point-close-btn').on('click', function () {
          $("#openSPU_start-point-close-btn").fadeOut();
        });
      })(jQuery);
    </script>



