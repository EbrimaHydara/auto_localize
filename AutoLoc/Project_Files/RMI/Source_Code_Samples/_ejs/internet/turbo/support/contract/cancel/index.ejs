<% const pagetitle = "ご解約について" -%>
<% const directories = [{path:"internet/turbo/support/", label:"お客様サポート"}, {path:"internet/turbo/support/contract/", label:"契約内容・お手続き"}] -%>
<% const namespace = "support-smallcategory" -%>
<%
  const pageInfo = {
    namespace,
    ancList: [
      {
        path: '#cancellation01',
        name: '回線の利用をやめて解約を行う場合'
      },
      {
        path: '#cancellation02',
        name: 'ご契約者がお亡くなりになった場合',
      },
      {
        path: '#cancellation03',
        name: '初期契約解除を行いたい',
      },
      {
        path: '#cancellation04',
        name: 'お申し込みをキャンセルしたい（利用開始前）',
      }
